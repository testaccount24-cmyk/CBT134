//GREGEXA  JOB (),G.PRICE,CLASS=A,MSGCLASS=X,MSGLEVEL=(1,1),
//             NOTIFY=&SYSUID.,REGION=6M,COND=(0,NE)
//*
//*  ASSEMBLE AND LINK THE REVEXSYM PROGRAM
//*
//*  REVEXSYM IS USED TO POST-PROCESS THE OBJECT DECK (MOSTLY
//*  SYM CARDS) AND THE LISTING PRODUCED BY ASSEMBLING NUMEROUS
//*  SMF RECORD DSECTS - THE OUTPUT FROM WHICH IS THE ASSEMBLER
//*  SOURCE OF THE REVSMFZ DATA-ONLY MODULE.
//*
//*  THERE IS NO NEED TO PLACE REVEXSYM IN THE LINKLIST.
//*
//*  RERUN THE REVEXSYM PROGRAM TO MAKE A NEW REVSMFZ MODULE
//*  WHENEVER MAINTENANCE OR UPGRADES CHANGE SMF RECORD STRUCTURES.
//*
//*  REVEXSYM ITSELF ONLY NEEDS UPDATING WHENEVER THE SUPPORT
//*  FOR ADDITIONAL SMF RECORD SECTIONS IS NEEDED.  ADDITIONAL
//*  ITEMS ADDED TO KNOWN SECTIONS DOES NOT USUALLY REQUIRE
//*  REVEXSYM CHANGES UNLESS YOU WANT TO FORCE A PARTICULAR
//*  DISPLAY FORMAT.
//*
//ASM     EXEC PGM=ASMA90,PARM='TERM,NODECK,OBJECT,XREF(SHORT)'
//SYSTERM  DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=SYS1.MODGEN,DISP=SHR
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(10,2))
//SYSLIN   DD  DSN=&&OBJSET,DISP=(NEW,PASS),UNIT=VIO,
//             SPACE=(CYL,(1,1)),
//             DCB=(DSORG=PS,RECFM=FB,LRECL=80,BLKSIZE=4080)
//SYSIN    DD  DSN=&SYSUID..REVIEW.ASM(REVEXSYM),DISP=SHR
//*
//LKED    EXEC PGM=IEWL,PARM='LIST,MAP'
//SYSPRINT DD  SYSOUT=*
//SYSLIN   DD  DSN=&&OBJSET,DISP=(OLD,DELETE)
//SYSLIB   DD  DSN=SYS1.CSSLIB,DISP=SHR
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(1,1))
//SYSLMOD  DD  DSN=&SYSUID..TEST.LOAD(REVEXSYM),DISP=SHR
//
